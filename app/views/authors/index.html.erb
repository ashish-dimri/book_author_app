<h1 class="jumbotron"> Authors </h1>
<div class="container">
  <div class="row">
    <%= link_to 'Add Author', new_author_path %>
    <div class="col-12 card">
      <table class="table">
			  <thead>
			    <th> First Name </th>
			    <th> Last Name </th>
			    <th> DOB </th>
			  </thead>
			  <tbody>
			  	<%= render partial: "author", collection: @authors %>
			  </tbody>
			</table>
    </div>
  </div>
</div>
